class MainProgram {
  public static void main(String[] args) {
    // Create an object of class A
    A a = new A();
    a.print();

    // Create an object of class B
    B b = new B();
    b.print();
  }
}

class A {
  public void print() {
    System.out.println("This is class A");
  }
}

class B {
  public void print() {
    System.out.println("This is class B");
  }
}
